{
  "build": {
    "builder": "dockerfile",
    "dockerfilePath": "KaizenekaApi/Dockerfile",
    "buildCommand": "docker build -t kaizeneka-api ."
  },
  "deploy": {
    "startCommand": "docker run -p $PORT:10000 kaizeneka-api",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}